<center><h1>New Employee</h1></center>
<center>
<%= form_for :employee do |f| %>
  <p>
    <%= f.label :PRAN_Number %><br>
    <%= f.text_field :text %>
  </p>
 
  <p>
    <%= f.label :Employee_Name %><br>
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.label :Employee_ID %><br>
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.label :NLCC %><br>
    <%= f.text_area :text %>
  </p>
 <p>
    <%= f.label :Remarks %><br>
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit %>
  </p>
</center>
<% end %>